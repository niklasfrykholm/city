string(TIMESTAMP TimeStamp %y%m%d_%H%M%S)

execute_process(
	COMMAND ${CMAKE_COMMAND} -E rename city_pluginw64_debug.dll city_pluginw64_debug.dll.old
	COMMAND ${CMAKE_COMMAND} -E rename city_pluginw64_debug.pdb "city_pluginw64_debug.${TimeStamp}.pdb.old"
	WORKING_DIRECTORY dlls
)

file(GLOB ToDelete dlls/*.old)
foreach(ToDeleteFile ${ToDelete})
	execute_process(
		COMMAND ${CMAKE_COMMAND} -E remove ${ToDeleteFile}
		ERROR_QUIET
	)
endforeach(ToDeleteFile)
